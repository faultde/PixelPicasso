<div class="jumbotron jumbotron-fluid bg-transparent">
  <div class="container">
    <h1 class="display-4">Availabe User Images</h1>
  </div>
</div>

  <div class="container">
    <div class="row">

        <!-- EDITOR BODY -->

   <% for(var i=0; i<container.length; i++){ %>
        <div class="card shadow mb-3 bg-white rounded m-2">
        <div class="card-body m-1">
        <h5 class="card-title"> User Image: <%=i%></h5>
        <div class="flex-container-300">

            <% for(var j=0; j< container[i].color.length; j++){ %>
            <% if(container[i].color.length == 100 ){%>
              <div class="block border  <%= container[i].color[j] %>" style="width: 10%; height: 10%;"></div>
            <% }else{ %>
              <div class="block border  <%= container[i].color[j]  %>" style="width: 5%; height: 5%;"></div>
            <% } %>  
            <% } %>
            
            <p>Created by:<%= container[i]._id %></p>
            
        </div>
        </div>
        </div>
          
  <% } %>
  </div>
  </div>







  <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js'></script>






